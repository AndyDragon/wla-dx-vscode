	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#080"
			}
		},{
			"scope"	: "comment.block.disabled",
			"settings": {
				"foreground": "#0808",
				"fontStyle": "italic"
			}
		},{
			"scope": "string",
			"settings": {
				"foreground": "#800"
			}
		},{
			"scope": "keyword",
			"settings": {
				"foreground": "#008",
				"fontStyle": "bold"
			}
		},{
			"scope": [
				"storage.type",
				"entity.name.function.macro"
			],
			"settings": {
				"foreground": "#00f",
				"fontStyle": "bold"
			}
		},{
			"scope" : "support.type.property-name",
			"settings": {
				"foreground": "#008",
				"fontStyle": "bold"
			}
		},{
			"scope" : "constant",
			"settings": {
				"foreground": "#808",
				"fontStyle": "bold"
			}
		},{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#808",
				"fontStyle": ""
			}
		},{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#08f",
				"fontStyle": "bold"
			}
		},{
			"scope": [
				"entity.name.method",
				"entity.name.property"
			],
			"settings": {
				"foreground": "#08f",
				"fontStyle": "italic"
			}
		},{
			"scope" : "entity.name.type",
			"settings" : {
				"foreground" : "#08f",
				"fontStyle": ""
			}
		},{
			"scope" : "entity.name.type.property",
			"settings" : {
				"fontStyle": "italic"
			}
		},{
			"scope" : "storage.modifier",
			"settings": {
				"foreground": "#00f",
				"fontStyle": "italic bold"
			}
		},{
			"scope": "support.function",
			"settings": {
				"foreground": "#008",
				"fontStyle": "bold"
			}
		},{
			"scope": "support.function.illegal",
			"settings": {
				"foreground": "#c00",
				"fontStyle": "bold"
			}
		},{
			"scope": "support.variable",
			"settings": {
				"foreground": "#f80",
				"fontStyle": "bold"
			}
		},{
			"scope": "support.variable.register",
			"settings": {
				"foreground": "#f80",
				"fontStyle": "bold"
			}
		},{
			"scope": "support.variable.register.shadow",
			"settings": {
				"foreground": "#960",
				"fontStyle": "bold"
			}
		},{
			"scope": "support.constant",
			"settings": {
				"foreground": "#f08",
				"fontStyle": "bold"
			}
		},{
			"scope" : "keyword.operator",
			"settings": {
				"foreground": "#88c"
			}
		},{
			"scope" : "punctuation",
			"settings": {
				"foreground": "#448"
			}
		},{
			"scope" : "punctuation.separator",
			"settings": {
				"foreground": "#bbb"
			}
		}
	]
}