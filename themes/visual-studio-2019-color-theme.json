	"tokenColors": [
		{
			"scope"		: "comment",
			"settings"	: { "foreground"	: "#008000" }
		},{
			"scope"		: "string",
			"settings"	: { "foreground"	: "#A31515" }
		},{
			"scope" 	: [
				"keyword", "storage.type", "storage.modifier", "support.function",
				"constant.language"
			],
			"settings"	: { "foreground"	: "#00f" }
		},{
			"scope"		: [ "constant", "keyword.operator" ],
			"settings"	: { "foreground"	: "#000" }
		},{
			"scope"		: [	"entity", "storage", "variable" ],
			"settings"	: { "foreground"	: "#000" }
		},{
			"scope"		: [ "entity.name", "entity.type", "support.type" ],
			"settings"	: {	"foreground"	: "#2B91AF"}
		},{
			"scope"		: [ "meta.preprocessor", "meta.preprocessor keyword" ],
			"settings"	: { "foreground"	: "#808080" }
		},{
			"scope"		: "meta.preprocessor",
			"settings"	: { "foreground"	: "#808080" }
		}
		
	]
